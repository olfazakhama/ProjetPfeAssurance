{% block title %}Compagnie{% endblock %}

{% block body %}
    <h1>prime index</h1>


  
<body class="hold-transition sidebar-mini">
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Listes des  primes</h1>
          </div>
         
        </div>
      </div><!-- /.container-fluid -->
            <div class="card">
              <div class="card-header">
                <h3 class="card-title">primes</h3>
              </div>
              
              <!-- /.card-header -->
              <div class="card-body">
               
                    <div class="card-body">
                      <table id="example1" class="table table-bordered table-striped">

                        <thead>
                        <tr>
             <th>MontantInitiale</th>
                <th>MontantFrequence</th>
                <th>MontantPayer</th>
                <th>Anné</th>
                <th>actions</th>
                          
                        </tr>
                        </thead>
                        <tbody>
                         
                            {% for prime in primes %}
                          <tr>
                            <td class="sorting_1 dtr-control" tabindex="0">{{ prime.montantInitiale }}</td>
                           
            
                <td>{{ prime.montantFrequence }}</td>
                <td>{{ prime.montantPayer }}</td>
                <td>{{ prime.annee }}</td>
                          
                            <td>
                                <a class="btn btn-info btn-sm" href="{{ path('prime_edit', {'id': prime.id}) }}">
                                    <i class="fas fa-pencil-alt">
                                         </i>
                                   Modifier
                                </a>
                                {{ include('prime/_delete_form.html.twig') }}
                            </td>
                        </tr>
                        {% endfor %}
                        </tbody>
                        <tfoot>
                        <tr>
               <th>MontantInitiale</th>
                <th>MontantFrequence</th>
                <th>MontantPayer</th>
                <th>Année</th>
                <th>actions</th>
                        </tr>
                        </tfoot>
                      </table>
                    </div>
                  </tfoot>
                </table>
                   <a class="btn btn-success btn-sm" href="{{ path('prime_new') }}">
                <i class="fa fa-plus" aria-hidden="true"></i>
           Ajouter un prime
          </a>
              </div>
           
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
 

  <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">
